@model IEnumerable<DSupportWebApp.Models.asis_param>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IDParam)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.KeyNL)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.KeyEN)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Value)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IDUserCreated)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IDUserModified)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateCreated)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateModified)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.IDParam)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.KeyNL)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.KeyEN)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Value)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IDUserCreated)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IDUserModified)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateCreated)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateModified)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.IDParam }) |
            @Html.ActionLink("Details", "Details", new { id = item.IDParam }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.IDParam })
        </td>
    </tr>
}

</table>
